<div id="results" v-cloak>
<div class="container">
    <h2 class="heading">ENTER RESULTS BY ROOM</h2>
    <div class="row">
      <div class="col-md">
        <table class="table table-striped table-bordered">
          <thead>
            <tr>
              <th scope="col"> # </th>
              <th scope="col"> Venue </th>
              <th scope="col"> Government </th>
              <th scope="col"> Opposition </th>
              <th scope="col"> Adjudicator </th>
              <th scope="col"> Results </th>
            </tr>
          </thead>
          <tbody>
           <% matchups.forEach(function(matchup, index) { %>
                <tr>
                    <td> <%= index + 1 %> </td>
                    <td> <%= matchup.venueName %> </td>
                    <td> <%= matchup.govTeamName %> </td>
                    <td> <%= matchup.oppTeamName %> </td>
                    <td> <%= matchup.adjName %> </td>
                    <td> <a href="/enter-results?matchupId=<%= matchup.matchupId %>"> Enter results </a> </td>
                </tr>
            <% }) %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
<%- /* Expose server-rendered data as window.SAILS_LOCALS :: */ exposeLocalsToBrowser() %>
