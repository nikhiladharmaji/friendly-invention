<div id="participants" v-cloak>
  <div class="container">
    <div class="row">
      <div class="col-md">
        <table class="table table-striped table-bordered">
          <thead>
            <tr>
              <th scope="col"> # </th>
              <th scope="col"> Name </th>
              <th scope="col">Team </th>
            </tr>
          </thead>
          <tbody>
           <% speakers.forEach(function(speaker, index) { %>
                <tr>
                    <td> <%= index + 1 %> </td>
                    <td> <%= speaker.name %> </td>
                    <td> <%= speaker.teamName %> </td>
                </tr> 
            <% }) %>
          </tbody>
        </table>
      </div>

      <div class="col-md">
        <table class="table table-striped table-bordered">
          <thead>
            <tr>
              <th scope="col"> # </th>
              <th scope="col"> Name </th>
              <th scope="col">Team </th>
            </tr>
          </thead>
          <tbody>
           <% speakers.forEach(function(speaker, index) { %>
                <tr>
                    <td> <%= index + 1 %> </td>
                    <td> <%= speaker.name %> </td>
                    <td> <%= speaker.teamName %> </td>
                </tr> 
            <% }) %>
          </tbody>
        </table>
      </div>

    </div>
  </div>
</div>
<%- /* Expose server-rendered data as window.SAILS_LOCALS :: */ exposeLocalsToBrowser() %>
